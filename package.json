{
  "name": "getir-challenge",
  "version": "1.0.0",
  "description": "A RESTful API with a single endpoint that fetches the data in the provided MongoDB collection and return the results in the requested format",
  "main": "src/app.js",
  "type": "module",
  "private": true,
  "engines": {
    "npm": ">=7.19.1",
    "node": "~14.17.4"
  },
  "scripts": {
    "start": "node src/app.js",
    "lint": "eslint --ext .js,.ts .",
    "pretest": "npm run lint",
    "test": "mocha ./**/*.test.js --ignore './node_modules/**/*'",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "npm run lint -- --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amalfra/getir-challenge.git"
  },
  "author": "Amal Francis <amalfra@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amalfra/getir-challenge/issues"
  },
  "homepage": "https://github.com/amalfra/getir-challenge",
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-strongloop": "^2.1.0",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "mocha": "^9.0.3",
    "mocked-env": "^1.3.5",
    "prettier": "^2.3.2"
  },
  "dependencies": {
    "@joi/date": "^2.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "joi": "^17.4.2",
    "mongoose": "^5.13.5"
  }
}
